{
  "name": "livecodelabluby",
  "version": "1.0.0",
  "description": "Preparação para a prova de Live Code do Lab Luby",
  "main": "index.js",
  "repository": "git@github.com:deibsoncogo/LiveCodeLabLuby.git",
  "author": "Deibson Cogo <deibsoncogo@outlook.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "db": "yarn db:revert && yarn db:run && yarn dev",
    "db:show": "yarn typeorm migration:show",
    "db:create": "yarn typeorm migration:create -n",
    "db:run": "yarn typeorm migration:run",
    "db:revert": "yarn typeorm migration:revert",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "dev": "ts-node-dev --inspect --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "test": "jest",
    "test:watch": "jest watch",
    "test:db": "yarn db:revert && yarn db:run && yarn test"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.16.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "factory-girl": "^5.0.4",
    "faker": "^6.6.6",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  }
}
